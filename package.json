  "scripts": {
    "ng": "ng",
    "start": "export NODE_OPTIONS=--max-old-space-size=4096 && export NODE_OPTIONS=--openssl-legacy-provider && ng serve --poll",
    "hmr": "ng serve --hmr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:tailwind": "tailwind build -i src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm start"
  },
